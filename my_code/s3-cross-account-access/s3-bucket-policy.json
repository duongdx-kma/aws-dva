{
    "Version": "2012-10-17",
    "Id": "PutObjPolicy",
    "Statement": [
        {
            "Sid": "allow",
            "Effect": "Allow",
            "Principal": {
                "AWS": [
                    "arn:aws:iam::240993297305:root",
                    "arn:aws:iam::240993297305:user/duongdx-dev",
                    "arn:aws:iam::195211018705:root"
                ]
            },
            "Action": [
                "s3:ListBucket",
                "s3:GetBucketLocation"
